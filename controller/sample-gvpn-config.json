{
  "CFx": {
    "Model": "GroupVPN"
  },
  "Logger": {
    "Enabled": true,
    "LogLevel": "WARNING",
    "LogOption": "File",
    "LogFilePath": "./logs/",
    "CtrlLogFileName": "ctrl.log",
    "TincanLogFileName": "tincan.log",
    "LogFileSize": 5000000,
    "BackupLogFileCount": 5
  },
  "TincanInterface": {
    "ctrl_recv_port": 5801,  
    "localhost": "127.0.0.1",
    "ctrl_send_port": 5800,  
    "localhost6": "::1",
    "dependencies": ["Logger"],
    "Vnets": [
      {
        "Name" : "ipop_tap0",
        "IP4": "10.254.0.10",
        "IP4PrefixLen": 16,
        "MTU4": 1200,
        "XMPPModuleName": "XmppClient",
        "TapName": "ipop_tap0",
        "Description": "Beta 2 Test Network",
        "IgnoredNetInterfaces": [ "ipop_tap0" ],
        "L2TunnellingEnabled": true
      }
    ],
    "Stun": [ "stun.l.google.com:19302" ]
  },
  "XmppClient": {
    "XmppDetails": [
      {
        "Username": "***",
        "Password": "***",
        "AddressHost": "***",
        "Port": "5222",
        "TapName": "ipop_tap0",
        "AuthenticationMethod": "password",
        "AcceptUntrustedServer": true
      }
    ],
    "TimerInterval": 10
  }
}
